{
  "name": "lch/admin-bundle",
  "type": "symfony-bundle",
  "description": "Bring admin feature to Symfony. Use other LCH bundles",
  "keywords": ["admin", "interface"],
  "homepage": "https://github.com/compagnie-hyperactive/AdminBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas BONNIOT",
      "email": "nicolas@devgiants.fr"
    }
  ],
  "require": {
    "php": "^5.5.9 || ^7.0",
    "symfony/symfony": "~3.1",
    "friendsofsymfony/jsrouting-bundle": "^1.6",
    "knplabs/knp-menu-bundle": "^2.1",
    "lch/components-bundle": "0.5",
    "lch/menu-bundle": "0.5",
    "lch/seo-bundle": "0.5",
    "lch/media-bundle": "1.0.1"
  },
  "require-dev": {
    "doctrine/doctrine-bundle": "^1.3",
    "phpunit/phpunit": "~4.8|~5.0"
  },
  "conflict": {
    "symfony/doctrine-bridge": "<2.7"
  },
  "autoload": {
    "psr-4": {
      "Lch\\AdminBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
}
